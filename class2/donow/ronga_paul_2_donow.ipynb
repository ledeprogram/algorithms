    "from math import pi\n",
    "\n",
    "# PI = 3.14159265359\n",
    "# volume = 4/3 Ï€ r3\n",
    "\n",
    "def get_volume(r):\n",
    "    if r < 0:\n",
    "        return False\n",
    "    return (4/3) * pi * (r ** 3)\n",
    "\n",
    "get_volume(30)"
   ]
