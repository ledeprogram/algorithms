    "def get_mean(numbers_list):\n",
    "    s = numbers_list[0]\n",
    "    for element in numbers_list[1:]:\n",
    "        s += element\n",
    "    return s/len(numbers_list)\n",
    "get_mean([2, 2, 3, 10, 5])\n",
    "\n"
   ]
