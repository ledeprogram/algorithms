    "def list_to_mean(list):\n",
    "    added_tog = 0\n",
    "    how_many = 0\n",
    "    for number in list:\n",
    "        added_tog += number\n",
    "        how_many += 1\n",
    "    return added_tog / how_many"
   ]
  }
