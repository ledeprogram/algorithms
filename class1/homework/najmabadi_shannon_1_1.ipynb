    "def my_mean(input_list):\n",
    "    sum_list = 0\n",
    "    count_list = 0\n",
    "    \n",
    "    for i in input_list:\n",
    "        sum_list += i\n",
    "        count_list += 1\n",
    "    \n",
    "    return sum_list/count_list"
   ]
