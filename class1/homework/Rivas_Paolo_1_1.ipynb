    "def stats(x):\n",
    "    n = 0\n",
    "    m = 0\n",
    "    for x_i in x:\n",
    "        n = n + 1\n",
    "        m = m + (x_i - m) / n\n",
    "    return {'mean': m}\n"
   ]
